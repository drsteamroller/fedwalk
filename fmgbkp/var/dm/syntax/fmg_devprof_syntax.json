{
	"macro": {
		"DOM_GLOBAL":  1,
		"DOM_VDOM":    2,

		"PKG_TYPE": {
			"devprof": 0
		},

		"ENABLED_PAGES": {
			"none": 0,
			"dns": 2,
			"ntp": 4,
			"email": 8,
			"admin": 16,
			"snmp": 32,
			"repmsg": 64,
			"ftgd": 128,
			"log": 256
		}
	},
	"obj": {
		"_pkg_obj": {
			"domain": "$DOM_VDOM",
			"mapping_name": "",
			"type": "table",
			"mkey": "name",
			"attr": {
				"name": {
					"type": "string",
					"sz": 64
				},
				"oid": {
					"type": "uint64",
					"read_only": 1
				},
				"type": {
					"type": "uint32",
					"opts": "$PKG_TYPE"
				},
				"description": {
					"type": "string",
					"sz": 255
				},
				"enabled options": {
					"type": "uint32",
					"opts": "$ENABLED_PAGES",
					"excluded": "false"
				},
				"scope member": {
					"type": "scope object",
					"extref": [ "/dvmdb/device", "/dvmdb/group" ]
				},
				"subobj": {
					"type": "table",
					"refer": "/pm/devprof",
					"internal": 1
				}
			}
		}
	}
}
