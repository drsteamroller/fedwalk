{
	"macro": {
		"DOM_GLOBAL":  1,
		"DOM_VDOM":    2,

		"META_IMPORTANCE": {
			"optional": 0,
			"required": 1
		},

		"ENABLE_OPTS": {
			"disable": 0,
			"enable": 1
		},
	},
	"obj": {
		"system/status": {
			"domain": "$DOM_GLOBAL",
			"type": "obj",
			"attr": {
				"Platform Type": {"type": "string"},
				"Platform Full Name": {"type": "string"},
				"Version": {"type": "string"},
				"Serial Number": {"type": "string"},
				"BIOS version": {"type": "string"},
				"Hostname": {"type": "string"},
				"Max Number of Admin Domains": {"type": "string"},
				"Max Number of Device Groups": {"type": "string"},
				"Admin Domain Configuration": {"type": "string"},
				"HA Mode": {"type": "string"},
				"Branch Point": {"type": "string"},
				"Release Version Information": {"type": "string"},
				"Current Time": {"type": "string"},
				"Daylight Time Saving": {"type": "string"},
				"Time Zone": {"type": "string"},
				"x86-64 Applications": {"type": "string"},
				"Disk Usage": {"type": "string"},
				"License Status": {"type": "string"}
			}
		},
		"system/performance": {
			"domain": "$DOM_GLOBAL",
			"type": "obj",
			"subobj": {
				"CPU": {"type": "obj"},
				"Flash Disk": {"type": "obj"},
				"Hard Disk": {"type": "obj"},
				"Memory": {"type": "obj"}
			}
		},
		"exec/fgfm/reclaim-dev-tunnel": {
			"domain": "$DOM_GLOBAL",
			"type": "command",
			"attr": {
				"flags": {
					"type": "uint32",
					"opts": { 
						"force": 1,
						"get_info": 2
					},
					"excluded": false
				}
			},
			"response": {
				"assigned_ip": {"type": "string"},
				"managed_serial": {"type": "string"},
				"platform_str": {"type": "string"},
				"serialno": {"type": "string"},
				"version": {"type": "int32"},
				"build": {"type": "int32"},
				"branch_pt": {"type": "int32"}
			}
		},
		"exec/top": {
			"domain": "$DOM_GLOBAL",
			"type": "command",
			"internal": 1,
			"attr": {
				"top-n": {"type": "uint32"},
				"order-by": {"type": "string"}
			},
			"response": {
				"lists": {"type": "obj"},
				"summary": {"type": "obj"},
				"timestamp": {"type": "int32"}
			}
		},
		"exec/iotop": {
			"domain": "$DOM_GLOBAL",
			"type": "command",
			"internal": 1,
			"attr": {
				"top-n": {"type": "uint32"}
			},
			"response": {
				"lists": {"type": "obj"},
				"summary": {"type": "obj"},
				"timestamp": {"type": "int32"}
			}
		},
		"exec/killprocess": {
			"domain": "$DOM_GLOBAL",
			"type": "command",
			"internal": 1,
			"attr": {
				"pid": {"type": "int32"}
			},
			"response": {
				"timestamp": {"type": "int32"}
			}
		},
		"_meta_fields/system/admin/user": {
			"domain": "$DOM_GLOBAL",
			"type": "table",
			"no_key": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"importance": {
					"type": "int32",
					"opts": "$META_IMPORTANCE"
				},
				"length": {
					"type": "int32"
				},
				"status": {
					"type": "int32",
					"opts": "$ENABLE_OPTS"
				}
			}
		}
	}
}
