{
	"macro": {
		"DOM_GLOBAL":  1,
		"DOM_VDOM":    2,

		"SUPPORT_M_NO_DOMAIN": 8,
		"SUPPORT_M_GLOBAL": 16,

		"NO_KEY":   -1,
		"KEY_PATHNAME": 3,

		"ECPROF_EXPORT_TYPE": {
			"winmac": 0,
			"android": 1,
			"ios": 2
		},

		"CONTROLLER_TYPES": {
			"wtp": 1,
			"fext": 2,
			"fsw": 4 
		},

		"CONTROLLER_IMPORT_OPTS": {
			"import-all": 1,
			"create-task": 2,
			"auto-assign": 4,
			"create-log": 8
		},

		"META_IMPORTANCE": {
			"optional": 0,
			"required": 1
		},

		"ADOM_UPGRADE_OPTS": {
			"same-version": 1,
			"from-cli": 2,
			"dryrun": 4,
			"downgrade": 8
		},

		"ENABLE_OPTS_DISABLE": 0,
		"ENABLE_OPTS_ENABLE": 1,

		"ENABLE_OPTS": {
			"disable": "$ENABLE_OPTS_DISABLE",
			"enable": "$ENABLE_OPTS_ENABLE"
		},

		"AVAILABLE_OPTS_UNAVAIL": 0,
		"AVAILABLE_OPTS_AVAIL": 1,

		"AVAILABLE_OPTS": {
			"unavailable": "$AVAILABLE_OPTS_UNAVAIL",
			"available": "$AVAILABLE_OPTS_AVAIL"
		},

		"PKG_TYPE_OPTS": {
			"policy": 1,
			"devprof": 2,
			"ecprof": 3,
			"crprof": 4,
			"cert": 5,
			"wtp": 6,
			"wanprof": 7,
			"fsp": 8,
			"fext": 10,
			"template": 12,
			"fwmprof": 14,
			"tmplgrp": 15,
			"fab-auth": 17
		},

		"PKG_STYPE_OPTS": {
			"_ipsec": 1,
			"_router_static": 2,
			"_cst": 3,
			"_sdwan_overlay": 4,
			"_ips_setting": 5,
			"_fap_setting": 6,
			"router_bgp": 1240,
			"gl_ips_sensor": 2908
		}
	},
	"obj": {
		"_upgrade": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"attr": {
				"flags": {
					"type": "int32",
					"opts": "$ADOM_UPGRADE_OPTS",
					"default": 0,
					"internal": 1
				},
				"version": {
					"type": "int32",
					"internal": 1
				},
				"mr": {
					"type": "int32",
					"internal": 1
				}
			},
			"response": {
				"task": {
					"type": "datasrc",
					"extref": "/task/task",
					"limit": 1
				}
			}
		},
		"_reset/database": {
			"type": "command",
			"domain": "$DOM_GLOBAL",
			"attr": {
				"version": {
					"type": "int32"
				},
				"mr": {
					"type": "int32"
				}
			}
		},
		"_workspace/lock": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"attr": {
			},
			"response": {
			}
		},
		"_workspace/unlock": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"attr": {
			},
			"response": {
			}
		},
		"_workspace/commit": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"attr": {
			},
			"response": {
			}
		},
		"_adom/options": {
			"type": "obj",
			"domain": "$DOM_VDOM",
			"attr": {
				"assign_excluded": {
					"type": "int32"
				},
				"specify_assign_pkg_list": {
					"type": "int32"
				},
				"package list": {
					"type": "scope object",
					"extref": "/pm/pkg"
				}
			}
		},
		"_category/list": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1,
			"attr": {
				"id": {
					"type": "int32"
				},
				"name": {
					"type": "string"
				}
			}
		},
		"_application/list": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1,
			"attr": {
				"id": {
					"type": "int32"
				},
				"name": {
					"type": "string"
				}
			}
		},
		"_rule/list": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1,
			"attr": {
				"id": {
					"type": "int32"
				},
				"name": {
					"type": "string"
				}
			}
		},
		"_fct/endpoint-control/profile": {
			"type": "obj",
			"domain": "$DOM_VDOM",
			"attr": {
				"ecprof": {
					"type": "datasrc",
					"extref": "/pm/ecprof",
					"limit": 1
				},
				"name": {
					"type": "datasrc",
					"extref": "/pm/config/endpoint-control/profile",
					"limit": 1
				},
				"type": {
					"type": "uint32",
					"opts": "$ECPROF_EXPORT_TYPE",
					"optional": 1
				}
			},
			"response": {
			}
		},
		"_fsp/managed-switch/platforms": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1,
			"internal": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"prefix": {
					"type": "string"
				},
				"ports": {
					"type": "array"
				},
				"poe_ports": {
					"type": "array"
				},
				"capability": {
					"type": "string"
				},
				"capability32": {
					"type": "string"
				},
				"poe-detection-type": {
					"type": "int32"
				},
				"max-allowed-trunk-members": {
					"type": "int32"
				}
			}
		},
		"_devprof/import": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"attr": {
				"devprof": {
					"type": "datasrc",
					"extref": "/pm/devprof",
					"limit": 1
				},
				"device": {
					"type": "datasrc",
					"extref": "/dvmdb/device",
					"limit": 1
				}
			},
			"response": {
			}
		},
		"_fsp/import/template": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"internal": 1,
			"attr": {
				"switch": {
					"type": "datasrc",
					"extref": "/pm/config/device/switch-controller/managed-switch",
					"limit": 1
				},
				"template": {
					"type": "datasrc",
					"extref": "/pm/config/adom/switch-controller/managed-switch",
					"limit": 1
				},
				"device": {
					"type": "datasrc",
					"extref": "/dvmdb/device",
					"limit": 1
				}
			},
			"response": {
			}
		},
		"_fsp/custom/command": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"internal": 1,
			"attr": {
				"switch": {
					"type": "datasrc",
					"extref": "/pm/config/device/switch-controller/managed-switch"
				},
				"command": {
					"type": "datasrc",
					"extref": "/pm/config/adom/switch-controller/custom-command",
					"limit": 1
				},
				"device": {
					"type": "datasrc",
					"extref": "/dvmdb/device",
					"limit": 1
				}
			},
			"response": {
				"task": {
					"type": "datasrc",
					"extref": "/task/task",
					"limit": 1
				}
			}
		},
		"_controller/import/batch": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"internal": 1,
			"attr": {
				"device": {
					"type": "datasrc",
					"extref": "/dvmdb/device",
					"limit": 1
				},
				"ctypes": {
					"type": "uint32",
					"opts": "$CONTROLLER_TYPES",
					"default": 0,
					"excluded": false
				},
				"flags": {
					"type": "uint32",
					"opts": "$CONTROLLER_IMPORT_OPTS",
					"default": 0,
					"excluded": false
				},
				"log-file": {
					"internal": 1,
					"type": "string"
				}
			},
			"subobj": {
				"import-wtp-list": {
					"type": "table",
					"internal": 1,
					"attr": {
						"wtp": {
							"type": "datasrc",
							"extref": "/pm/config/device/wireless-controller/wtp",
							"limit": 1
						},
						"template": {
							"type": "datasrc",
							"extref": "/pm/config/adom/wireless-controller/wtp-profile",
							"limit": 1
						}
					}
				},
				"import-fsw-list": {
					"type": "table",
					"internal": 1,
					"attr": {
						"switch": {
							"type": "datasrc",
							"extref": "/pm/config/device/switch-controller/managed-switch",
							"limit": 1
						},
						"template": {
							"type": "datasrc",
							"extref": "/pm/config/adom/switch-controller/managed-switch",
							"limit": 1
						}
					}
				}
			},
			"response": {
				"task": {
					"type": "datasrc",
					"extref": "/task/task",
					"limit": 1
				},
				"log-file": {
					"internal": 1,
					"type": "string"
				},
				"log": {
					"type": "string"
				}
			}
		},
		"_wanprof/import": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"internal": 1,
			"attr": {
				"template": {
					"type": "datasrc",
					"extref": "/pm/wanprof",
					"limit": 1
				},
				"device": {
					"type": "scope object",
					"extref": "/dvmdb/device",
					"limit": 1
				}
			},
			"response": {
			}
		},
		"_blueprint/info": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"internal": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"platform": {
					"type": "string"
				},
				"variables": {
					"type": "array"
				}
			}
		},
		"_generate/fgsp/config": {
			"type": "command",
			"domain": "$DOM_VDOM",
			"internal": 1,
			"attr": {
				"group": {
					"type": "datasrc",
					"extref": "/dvmdb/group",
					"limit": 1
				},
				"standalone-group-id": {
					"type": "uint32",
					"default": 1
				},
				"layer2-connection": {
					"type": "int32",
					"opts": "$AVAILABLE_OPTS",
					"default": "$AVAILABLE_OPTS_UNAVAIL"
				},
				"session-sync-dev": {
					"type": "string",
					"default": null
				},
				"encryption": {
					"type": "int32",
					"opts": "$ENABLE_OPTS",
					"default": "$ENABLE_OPTS_DISABLE"
				},
				"session-pickup": {
					"type": "int32",
					"opts": "$ENABLE_OPTS",
					"default": "$ENABLE_OPTS_ENABLE"
				},
				"session-pickup-nat": {
					"type": "int32",
					"opts": "$ENABLE_OPTS",
					"default": "$ENABLE_OPTS_ENABLE"
				},
				"session-pickup-connectionless": {
					"type": "int32",
					"opts": "$ENABLE_OPTS",
					"default": "$ENABLE_OPTS_ENABLE"
				},
				"override": {
					"type": "int32",
					"opts": "$ENABLE_OPTS",
					"default": "$ENABLE_OPTS_DISABLE"
				}
			},
			"response": {
			}
		},
		"_package/status": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"attr": {
				"type": {
					"type": "int32",
					"opts": "$PKG_TYPE_OPTS",
					"default": 1
				},
				"stype": {
					"type": "int32",
					"opts": "$PKG_STYPE_OPTS",
					"default": 0
				}
			},
			"response": {
				"dev": {
					"type": "string"
				},
				"vdom": {
					"type": "string"
				},
				"adom": {
					"type": "string"
				},
				"pkg": {
					"type": "string"
				},
				"type": {
					"type": "int32",
					"opts": "$PKG_TYPE_OPTS",
					"default": 1
				},
				"stype": {
					"type": "int32",
					"opts": "$PKG_STYPE_OPTS",
					"default": 0
				},
				"status": {
					"type": "string"
				}
			}
		},
		"_data/tablesize": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1
		},
		"_data/tablesize/fos": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1
		},
		"_data/tablesize/fmg": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1
		},
		"_data/tablesize/log": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1
		},
		"_data/tablesize/faz": {
			"type": "table",
			"domain": "$DOM_VDOM",
			"support_mode": "$SUPPORT_M_GLOBAL",
			"no_key": 1
		},
		"_meta_fields/firewall/address": {
			"type": "table",
			"support_mode": "$SUPPORT_M_NO_DOMAIN",
			"no_key": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"importance": {
					"type": "int32",
					"opts": "$META_IMPORTANCE"
				},
				"length": {
					"type": "int32"
				}
			}
		},
		"_meta_fields/firewall/addrgrp": {
			"type": "table",
			"support_mode": "$SUPPORT_M_NO_DOMAIN",
			"no_key": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"importance": {
					"type": "int32",
					"opts": "$META_IMPORTANCE"
				},
				"length": {
					"type": "int32"
				}
			}
		},
		"_meta_fields/firewall/service/custom": {
			"type": "table",
			"support_mode": "$SUPPORT_M_NO_DOMAIN",
			"no_key": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"importance": {
					"type": "int32",
					"opts": "$META_IMPORTANCE"
				},
				"length": {
					"type": "int32"
				}
			}
		},
		"_meta_fields/firewall/service/group": {
			"type": "table",
			"support_mode": "$SUPPORT_M_NO_DOMAIN",
			"no_key": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"importance": {
					"type": "int32",
					"opts": "$META_IMPORTANCE"
				},
				"length": {
					"type": "int32"
				}
			}
		},
		"_meta_fields/firewall/policy": {
			"type": "table",
			"support_mode": "$SUPPORT_M_NO_DOMAIN",
			"no_key": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"importance": {
					"type": "int32",
					"opts": "$META_IMPORTANCE"
				},
				"length": {
					"type": "int32"
				}
			}
		},
		"_meta_fields/firewall/central-snat-map": {
			"type": "table",
			"support_mode": "$SUPPORT_M_NO_DOMAIN",
			"no_key": 1,
			"attr": {
				"name": {
					"type": "string"
				},
				"importance": {
					"type": "int32",
					"opts": "$META_IMPORTANCE"
				},
				"length": {
					"type": "int32"
				}
			}
		}
	}
}
